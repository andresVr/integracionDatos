    <sql>INSERT INTO datawarehouse_f.dim_fecha (
ID_FECHA, 
FECHA, ANIO, MES, DIA,
FECHA_CARGA) 
(select distinct truncate(ID_FECHA/100,0),
cast(concat (cast(truncate(ID_FECHA/10000,0) as CHAR),"-",cast(truncate(ID_FECHA/100,0)-(truncate(ID_FECHA/10000,0)*100) as char),"-01") as date) ,
truncate(ID_FECHA/10000,0),
truncate(ID_FECHA/100,0)-(truncate(ID_FECHA/10000,0)*100),0,
now()
from 
datawarehouse.fac_detalle_factura 
where 
truncate(ID_FECHA/100,0) not in
	(select ID_FECHA from datawarehouse.dim_fecha));

	
delete from datawarehouse_f.fac_venta_acumulada  where
ID_FECHA in 
(select df.ID_FECHA from  dim_fecha df 
where df.DIA = 0 and df.MES &lt;&gt; 0);
	


INSERT INTO 
datawarehouse.fac_venta_acumulada 
(ID_FECHA, 
ID_LOCAL, 
ID_UBICACION, 
ID_PRODUCTO,
ID_SUBGRUPO, 
VALOR, VALOR_MES_1, 
YTD, YTD_1,
MTD, MT_1, 
FECHA_CARGA)
select 
truncate(ID_FECHA/100,0),ID_LOCAL,ID_UBICACION,ID_PRODUCTO,ID_SUBGRUPO,0,0,0,0,sum(PRECIO) as m,
	(select 
		sum(fa.PRECIO)
	from 
		fac_detalle_factura as fa
	where
		(truncate(fa.ID_FECHA/100,0)) = (select year(date_add(df.FECHA,INTERVAL -1 month))*100+month(date_add(df.FECHA,INTERVAL -1 month)) from dim_fecha as df where df.ID_FECHA = truncate(ff.ID_FECHA/100,0)) and
		fa.ID_LOCAL = ff.ID_LOCAL and  
		fa.ID_UBICACION = ff.ID_UBICACION and 
		fa.ID_PRODUCTO = ff.ID_PRODUCTO and
		fa.ID_SUBGRUPO = ff.ID_SUBGRUPO
	group by 
		truncate(fa.ID_FECHA/100,0),
		fa.ID_LOCAL, 
		fa.ID_UBICACION, 
		fa.ID_PRODUCTO,
		fa.ID_SUBGRUPO
	) as m_aa , now()
from 
fac_detalle_factura as ff
group by 
truncate(ID_FECHA/100,0),
ID_LOCAL, 
ID_UBICACION, 
ID_PRODUCTO,
ID_SUBGRUPO;



</sql>
